{
  "name": "celulas3d-app",
  "version": "1.0.0",
  "description": "Aplicativo móvel para visualização de células 3D",
  "main": "index.js",
  "scripts": {
    "build": "echo 'Building app...' && cp -r src/* www/",
    "start": "python -m http.server 8081 --directory www",
    "android": "npx cap run android",
    "build:android": "npm run build && npx cap sync android && npx cap open android"
  },
  "keywords": [
    "ionic",
    "capacitor",
    "células",
    "3d",
    "biologia"
  ],
  "author": "Gustavo",
  "license": "MIT",
  "dependencies": {
    "@capacitor/android": "^5.7.8",
    "@capacitor/core": "^5.0.0",
    "@capacitor/splash-screen": "^5.0.0"
  },
  "devDependencies": {
    "@capacitor/cli": "^5.0.0"
  }
}
